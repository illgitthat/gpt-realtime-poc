{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "voice-adamcbloom",
  "main": "src/worker.ts",
  "compatibility_date": "2026-01-24",
  "workers_dev": true,
  "assets": {
    "directory": "./public",
    "binding": "ASSETS",
    "html_handling": "auto-trailing-slash"
  },
  "observability": {
    "logs": {
      "enabled": true,
      "invocation_logs": true
    }
  },
  "routes": [
    {
      "pattern": "voice.adamcbloom.com",
      "custom_domain": true
    }
  ]
}
