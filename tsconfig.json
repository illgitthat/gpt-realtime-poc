{
    "compilerOptions": {
      "module": "ES2022",
      "target": "ES2022",
      "moduleResolution": "bundler",
      "lib": [
        "ES2022"
      ],
      "skipLibCheck": true,
      "strict": true,
      "noEmit": true,
      "isolatedModules": true,
      "allowSyntheticDefaultImports": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": [
      "src/**/*",
      "*.ts",
      "worker-configuration.d.ts"
    ]
}
